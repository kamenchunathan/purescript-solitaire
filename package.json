{
  "private": true,
  "devDependencies": {
    "esbuild": "^0.14.50",
    "parcel": "2.5.0",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "purescript": "0.15.0",
    "spago": "^0.19.1"
  },
  "scripts": {
    "build": "spago build",
    "test": "spago test",
    "clean": "rm -rf node_modules output dce-output .spago dist/* *.lock .cache",
    "serve": "parcel dev/index.html",
    "repl": "spago repl",
    "build-prod": "mkdir -p prod && cp dev/* prod/ && rm -rf dist && spago bundle-app --to prod/index.js && parcel build prod/index.html"
  },
  "staticFiles": {
    "staticPath": "./static"
  }
}
